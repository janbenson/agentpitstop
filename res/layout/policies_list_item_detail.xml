<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="center"
    android:background="@color/black"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/last"
        android:layout_alignLeft="@+id/last"
        android:text="@string/Last"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="@color/dark_blue" />
    
    <TextView
        android:id="@+id/first"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="36dp"
        android:paddingBottom="4sp"
        android:paddingLeft="8sp"
        android:paddingTop="8sp"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/last"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/first"
        android:layout_alignBottom="@+id/first"
        android:layout_toRightOf="@+id/first"
        android:paddingBottom="4sp"
        android:paddingLeft="8sp"
        android:paddingTop="8sp"
        android:textSize="20sp"
        android:textStyle="bold" />

     <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/company"
        android:text="@string/Plan"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="@color/dark_blue" />
    
    
    <TextView
        android:id="@+id/company"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView6"
        android:paddingBottom="6sp"
        android:paddingLeft="8sp"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/plan"
        android:text="@string/Status"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="@color/dark_blue" />
    

    <TextView
        android:id="@+id/plan"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView3"
        android:paddingBottom="4sp"
        android:paddingLeft="8sp"
        android:paddingTop="8sp"
        android:textSize="15sp"
        android:textStyle="bold" />

  <TextView
      android:id="@+id/textView5"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignParentLeft="true"
      android:layout_alignRight="@+id/status"
      android:layout_below="@+id/status"
      android:text="@string/submitted"
      android:textAppearance="?android:attr/textAppearanceSmall"
      android:textColor="@color/dark_blue" />
  
    <TextView
        android:id="@+id/status"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView4"
        android:paddingBottom="4sp"
        android:paddingLeft="8sp"
        android:paddingTop="8sp"
        android:textSize="15sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/submitted"
        android:layout_alignLeft="@+id/last"
        android:layout_marginLeft="14dp"
        android:text="@string/Effective"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="@color/dark_blue" />
    
    <TextView
        android:id="@+id/submitted"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView5"
        android:paddingBottom="6sp"
        android:paddingLeft="8sp"
        android:textSize="15sp" />



    <TextView
        android:id="@+id/effective"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_toRightOf="@+id/submitted"
        android:paddingBottom="6sp"
        android:paddingLeft="8sp" />

    <TextView
        android:id="@+id/txtsubmitted"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView5"
        android:paddingBottom="6sp"
        android:paddingLeft="8sp"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/last"
        android:layout_alignParentLeft="true"
        android:text="@string/First"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="@color/dark_blue" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/first"
        android:text="@string/Company"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/dark_blue" />

    <TextView
        android:id="@+id/TextView01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/submitted"
        android:text="@string/Email"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/dark_blue" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/TextView01"
        android:layout_alignBottom="@+id/TextView01"
        android:layout_alignLeft="@+id/textView7"
        android:text="@string/Phone"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/dark_blue" />

    <TextView
        android:id="@+id/email"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/TextView01"
        android:paddingBottom="6sp"
        android:paddingLeft="8sp"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/phone"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView10"
        android:layout_below="@+id/textView10"
        android:paddingBottom="6sp"
        android:paddingLeft="8sp"
        android:textSize="15sp" />
    
</RelativeLayout>